{
  "name": "h2h",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "^18.18.0 || ^20.9.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "ts-node --project tsconfig.server.json src/server.ts",
    "dev:worker": "wrangler dev --env development",
    "dev:cf": "concurrently \"npm run dev:worker\" \"next dev\"",
    "build": "next build",
    "start": "cross-env NODE_ENV=production ts-node --project tsconfig.server.json src/server.ts",
    "lint": "next lint",
    "setup-db": "ts-node -P tsconfig.scripts.json scripts/setup-db.ts",
    "migrate-profile": "ts-node -P tsconfig.scripts.json scripts/migrate-profile-system.ts",
    "migrate-heart-points": "ts-node -P tsconfig.scripts.json scripts/migrate-heart-points.ts",
    "migrate-message-eligibility": "ts-node -P tsconfig.scripts.json scripts/migrate-message-eligibility.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "@aws-sdk/client-ses": "^3.714.0",
    "@marsidev/react-turnstile": "^1.1.0",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-tooltip": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.13.0",
    "@vercel/postgres": "^0.10.0",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.456.0",
    "next": "15.0.3",
    "next-auth": "^4.24.10",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-idle-timer": "^5.7.2",
    "socket.io": "^4.7.4",
    "socket.io-client": "^4.7.4",
    "tailwind-merge": "^2.5.4",
    "tailwindcss-animate": "^1.0.7",
    "wrangler": "^3.94.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241112.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  }
}
